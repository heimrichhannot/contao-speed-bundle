{
  "name": "heimrichhannot/contao-speed-bundle",
  "type": "contao-bundle",
  "description": "This module improves the performance of your contao environment.",
  "license": "LGPL-3.0+",
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.3.1",
    "heimrichhannot-contao-components/lazyload": "^1.0"
  },
  "require-dev": {
    "contao/test-case": "^1.1",
    "contao/manager-plugin": "^2.0",
    "friendsofphp/php-cs-fixer": "^2.2",
    "phpunit/phpunit": "^5.6|^6.0",
    "satooshi/php-coveralls": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\SpeedBundle\\": "src/"
    },
    "classmap": [
      "src/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "HeimrichHannot\\SpeedBundle\\Test\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\SpeedBundle\\ContaoManager\\Plugin"
  }
}
